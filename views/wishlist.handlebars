<div class="row card-section">
{{#each wish}}
<div class="col-4">
<div class="card-deck">
    <div class="cards-container">
        <div class="card mb-3">
            <img src="{{this.url}}" class="card-img-top" alt="{{this.name}}">
            <div class="card-body">
                <h5 class="card-title" data-plant-id={{this.id}}>{{this.name}}</h5>
                <p class="card-text">{{this.scientific_name}}</p>
                <p class="card-text"><small class="text-muted"></small></p>
            <div class="card-footer">
                    <button class="btn details"><a href="/plant/{{this.id}}">Details</a></button>
                    <button class="btn btn-outline">Delete from Wishlist</button>
            </div>
            </div>
        </div>
    </div>
</div>
</div>
{{/each}}
</div>

<script src="/js/wish.js"></script>